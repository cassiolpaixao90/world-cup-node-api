  "description": "world cup + api + node + rest ",
  "scripts": {
    "prestart": "babel-node ./src/utils/utils.js",
    "start": "npm-run-all --parallel security-check open:src",
    "open:src":"nodemon --exec npm run babel-node -- src/clusters.js",
    "babel-node": "babel-node --presets=env",
    "test": "echo \"Error: no test specified\" && nexit 1",
    "test-integration": "NODE_ENV=test mocha --opts src/test/integration/mocha.opts test/integration/**/*.js",
    "security-check":"nsp check",
    "share": "lt --port 3000",
    "localtunnel": "npm-run-all --parallel open:src localtunnel"

  },
